<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="448.0" prefWidth="636.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.VentanaMenuController">
   <children>
      <GridPane alignment="CENTER" prefHeight="224.0" prefWidth="636.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="agregarLibrosButton" mnemonicParsing="false" onAction="#handleBooksButton" text="AgregarLibros" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="1">
               <children>
                  <Button fx:id="agregarAmigosButton" mnemonicParsing="false" onAction="#handleFriendsButton" text="AgregarAmigos" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2">
               <children>
                  <Button fx:id="agregarPrestamosButton" mnemonicParsing="false" onAction="#handleLoanButton" text="AgregarPrestamos" />
               </children>
            </HBox>
         </children>
      </GridPane>
      <Button fx:id="salirButton" mnemonicParsing="false" onAction="#handleExitButton" text="Salir" />
   </children>
</VBox>
